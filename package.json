{
  "name": "server",
  "version": "1.0.0",
  "description": "Fast-Food-Fastâ€‹ is a food delivery service app for a restaurant where you can place your order and get your food as soon as possible",
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": " nyc mocha --compilers js:babel-core/register --exit",
    "start": "nodemon --watch server --exec babel-node ./server.js",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "babel-node": "babel-node --presets=env",
    "start:dev": "npm run babel-node -- ./server.js",
    "build": "babel src -d dist",
    "mocha": "nyc mocha --compilers js:babel-register",
    "start:test": "nyc --reporter=html --reporter=text mocha --compilers js:babel-register --recursive ./**/**/*.test.js  --timeout=3000 --exit",
    "test-watch": "nodemon --exec 'npm test'",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Nwabuzor C Obiora",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "eslint-config-airbnb": "^17.1.0",
    "express": "^4.16.3",
    "mocha": "^5.2.0",
    "morgan": "^1.9.1",
    "supertest": "^3.3.0",
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-airbnb": "^2.5.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1"
  },
  "devDependencies": {
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "nyc": "^13.0.1"
  }
}
