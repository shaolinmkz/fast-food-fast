{
  "name": "server",
  "version": "1.0.0",
  "description": "Fast-Food-Fastâ€‹ is a food delivery service app for a restaurant where you can place your order and get your food as soon as possible",
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test && nyc --reporter=html --reporter=text mocha --require babel-register --timeout 10000 ./test/*.js --exit",
    "start": "babel-node ./server.js",
    "start:dev": "nodemon --watch server --exec babel-node ./server.js",
    "test:dev": "nyc --reporter=html --reporter=text mocha --compilers js:babel-register ./test --timeout 20000 --exit",
    "coveralls": "nyc npm test&& nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
