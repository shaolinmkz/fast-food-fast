    "express": "^4.16.3",
    "mocha": "^5.2.0",
    "morgan": "^1.9.1",
    "supertest": "^3.3.0",
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-airbnb": "^2.5.3",
    "babel-preset-env": "^1.7.0",
